config_version=5

[application]

config/name="Project-LocalMultiplayerPlatformer"
run/main_scene="res://Scenes/Arena.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=60
config/icon="res://icon.png"

[autoload]

